t.querySelector(\"#lite-ad-q4kttjg32k\");elem && elem.classList.remove('lite-page-ad--empty');}}})</script></div></div></div><section class=\"js-target-read publication-fulltext-section--redesign\" id=\"read\"><div class=\"nova-legacy-c-card nova-legacy-c-card--spacing-m nova-legacy-c-card--elevation-1-above publication-fulltext--redesign\"><div class=\"nova-legacy-c-card__header nova-legacy-c-card__header--show-divider nova-legacy-c-card__header--spacing-inherit\"><h2 class=\"nova-legacy-e-text nova-legacy-e-text--size-l nova-legacy-e-text--family-sans-serif nova-legacy-e-text--spacing-none nova-legacy-e-text--color-grey-700\">No full-text available</h2></div><div class=\"nova-legacy-c-card__body nova-legacy-c-card__body--spacing-inherit\"><div class=\"publication-no-fulltext--redesign\"><div class=\"request-fulltext-promo request-fulltext-promo--redesign\"><div class=\"nova-legacy-l-flex__item nova-legacy-l-flex nova-legacy-l-flex--gutter-m nova-legacy-l-flex--direction-column@s-up nova-legacy-l-flex--align-items-center@s-up nova-legacy-l-flex--justify-content-flex-start@s-up nova-legacy-l-flex--wrap-nowrap@s-up\"><div class=\"nova-legacy-l-flex__item request-fulltext-promo__image\"><picture width=\"60\" height=\"82\"> <img src=\"data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mN89OjNfwAJDwOxHEacwgAAAABJRU5ErkJggg==\" data-src=\"images/symbols/publication/publication_pdf_icon.png\" class=\"lite-image\" alt=\"Request Full-text Paper PDF\" width=\"60\" height=\"82\"/></picture></div><div class=\"nova-legacy-l-flex__item\"><p class=\"nova-legacy-e-text nova-legacy-e-text--size-m nova-legacy-e-text--family-sans-serif nova-legacy-e-text--spacing-none nova-legacy-e-text--color-grey-700\" align=\"center\">To read the full-text of this research,<br/> you can request a copy directly from the authors.</p></div><div class=\"nova-legacy-l-flex__item\"><button class=\"nova-legacy-c-button nova-legacy-c-button--align-center nova-legacy-c-button--r