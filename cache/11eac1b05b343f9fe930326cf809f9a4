acy.\",\"datePublished\":\"2017-10-10\",\"dateModified\":\"2017-10-10\",\"pageStart\":\"7275\",\"pageEnd\":\"7286\",\"sameAs\":\"https://doi.org/10.1007/s10586-017-1236-2\",\"keywords\":\"Processor Architectures,Operating Systems,Computer Communication Networks\",\"image\":\"https://static-content.springer.com/image/art%3A10.1007%2Fs10586-017-1236-2/MediaObjects/10586_2017_1236_Fig1_HTML.gif\",\"isPartOf\":{\"name\":\"Cluster Computing\",\"issn\":[\"1573-7543\",\"1386-7857\"],\"volumeNumber\":\"22\",\"@type\":[\"Periodical\",\"PublicationVolume\"]},\"publisher\":{\"name\":\"Springer US\",\"logo\":{\"url\":\"https://www.springernature.com/app-sn/public/images/logo-springernature.png\",\"@type\":\"ImageObject\"},\"@type\":\"Organization\"},\"author\":[{\"name\":\"Ali, Shaukat\",\"affiliation\":[{\"name\":\"Islamia College University\",\"address\":{\"name\":\"Islamia College University, Peshawar, Pakistan\",\"@type\":\"PostalAddress\"},\"@type\":\"Organization\"}],\"@type\":\"Person\"},{\"name\":\"Rauf, Azhar\",\"affiliation\":[{\"name\":\"University of Peshawar\",\"address\":{\"name\":\"University of Peshawar, Peshawar, Pakistan\",\"@type\":\"PostalAddress\"},\"@type\":\"Organization\"}],\"@type\":\"Person\"},{\"name\":\"Islam, Naveed\",\"affiliation\":[{\"name\":\"Islamia College University\",\"address\":{\"name\":\"Islamia College University, Peshawar, Pakistan\",\"@type\":\"PostalAddress\"},\"@type\":\"Organization\"}],\"@type\":\"Person\"},{\"name\":\"Farman, Haleem\",\"affiliation\":[{\"name\":\"Islamia College University\",\"address\":{\"name\":\"Islamia College University, Peshawar, Pakistan\",\"@type\":\"PostalAddress\"},\"@type\":\"Organization\"}],\"@type\":\"Person\"}],\"@type\":\"ScholarlyArticle\"},\"@context\":\"https://schema.org\",\"@type\":\"WebPage\"}</script>\n\n</head>\n<body class=\"shared-article-renderer\">\n    \n    \n    \n        \n            <!-- Google Tag Manager (noscript) -->\n            <noscript data-test=\"gtm-body\">\n            